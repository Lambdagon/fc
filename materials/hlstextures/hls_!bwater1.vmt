"LightmappedGeneric"
{
	"$basetexture" "hlstextures/hls_!bwater1"
    "$color" "[2 2 2]"
	"$alpha" "0.95"

        	"$refracttexture" "_rt_WaterRefraction"
	"$refractamount" "0"
        
	"%compilewater" 1
	"$surfaceprop" "water"
	"$bottommaterial" "hlstextures/hls_!bwater1_beneath"
	"$fogenable" 1
	"$fogcolor" "{ 24 28 32}"
	"$fogstart" 0.00
	"$fogend" 1400.00

	"$translate" "[0.0 0.0]"
	"$scale" "[1.5 1.5]"
	
	"Proxies"
	{
		"Sine"
		{
			"sineperiod" 6
			"sinemin" "1.5"
			"sinemax" "1.51"
			"resultVar" "$scale"
		}
		
		"Sine"
		{
			"sineperiod" 12
			"sinemin" "-.1"
			"sinemax" ".1"
			"resultVar" "$translate"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"scaleVar" "$scale"
			"resultVar" "$basetexturetransform"
		}
	}
}