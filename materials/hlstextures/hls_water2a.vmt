"LightmappedGeneric"
{
                "$abovewater" 0	
	"$basetexture" "hlstextures/hls_water2a"
	"$scale" "[.5 .5]"
	"$translucent" "1"
                "$alpha" "0.97"
	"$refracttexture" "_rt_WaterRefraction"
	"$refractamount" "0"
        
	"%compilewater" 1
	"$surfaceprop" "water"
                "$bottommaterial" "hlstextures/hls_water2a_beneath"
	"$fogenable" 1
	"$fogcolor" "{ 0 26 32 }"
	"$fogstart" 0.00
	"$fogend" 6000.00

	"$texoffset" "[0 0]"
	"$texscale" "1"
	

	"Proxies"
	{

		"TextureScroll"
		{
			"texturescrollvar" "$texoffset"
			"texturescrollrate" .20
			"texturescrollangle" 90.00
		}

		"TextureTransform"
		{
			"translateVar" "$texoffset"
			"scaleVar" "$texscale"
			"resultVar" "$basetexturetransform"
		}
	}
}