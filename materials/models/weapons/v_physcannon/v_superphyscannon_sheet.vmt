"VertexLitGeneric"
{
	"$baseTexture" "Models/Weapons/v_physcannon/v_superphyscannon_sheet"
	"$normalmap" "Models/Weapons/v_physcannon/v_superphyscannon_sheet_normal"
	"$phongexponenttexture" "Models/Weapons/v_physcannon/v_superphyscannon_sheet_ao"

	"$selfillum" 1

	"$phong" "0"
	"$phongexponent" "18"
	"$phongboost" "2"
	"$lightwarptexture" "models\weapons\lightwarp_tf2style\lightwarp_tf2style02"

	"$phongfresnelranges"	"[.5 .9 .3]"

	"$halflambert" "1"
	"$glowcolor" "2"

	"$tempvar" "0"

	"$commadd_phongexponent" "20"
	"$commadd_phongboost" 	"22.5"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "3"						
	"$rimlightexponent" "28"				
	"$rimlightboost" "3"

	"$yellow" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}