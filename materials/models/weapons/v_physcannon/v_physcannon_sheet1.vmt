"VertexlitGeneric"
{
	"$basetexture" "Models/Weapons/V_physcannon/v_physcannon_sheet1"
	"$selfillum" "1"
	"$envmap" "env_cubemap"
	"$envmapcontrast" ".7"
	"$envmapsaturation" "[.8 .8 .8]"
	"$normalmapalphaenvmapmask" 1

	// -- From here down is new stuff which will only be applied if $phong is set to 1 --
	"$phong" "0"
	"$lightwarptexture" "models\weapons\lightwarp_tf2style\lightwarp_tf2style02"
	"$phongexponent" "32"
	"$phongboost"	"4"
	"$phongfresnelranges"	"[.3 0.4 3.5]"

	"$yellow" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}