"VertexLitGeneric"
{
	"$basetexture"	"models\weapons\w_nailgun\w_nailgun01"
	"$bumpmap" "models\weapons\w_nailgun\w_nailgun_normal"
	"$phong" "1"
	"$phongexponenttexture" "models\weapons\w_nailgun\w_nailgun_exponent"
	"$phongboost" "10"
	
	"$lightwarptexture" "models\weapons\w_nailgun\w_nailgun_lightwarp"
	
	"$phongfresnelranges"	"[.5 1.5 10]"
	"$halflambert" "1"

	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
