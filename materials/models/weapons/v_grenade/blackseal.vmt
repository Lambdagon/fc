"vertexlitgeneric"
{
	"$bumpmap" "models/weapons/v_grenade/black seal_normal"
	//"$envmap" "env_cubemap"
	//"$envmapcontrast" 1
	//"$envmaptint" "[ .5 .5 .5 ]"
	"$baseTexture" "Models/Weapons/V_grenade/blackseal"


// -- From here down is new stuff which will only be applied if $phong is set to 1 --
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost"	"1"
	"$phongfresnelranges"	"[.2 0.4 2]"

	"360?$color2" "[ 0.4 0.4 0.4 ]"
	
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
	
}


}
