"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_smg/c_smg"
	"$phongexponenttexture" "models/weapons/c_models/c_smg/c_smg_exp"


	"$phong" "1"
	//commented out to use the exponent mask
	//"$phongexponent" "2"
	"$phongboost" "2.5"	
	
	//scales up the exponent texture which usually only has about 120 levels of exponent
	"$phongexponentfactor"  "120"
	"$phongalbedotint" "1"
	
	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"
			
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	"$glowcolor" "1"
	
	"$envmap" "env_cubemap"
	"$envmaptint" "[.05 .05 .05]"
	
	"$tempvar" "0"

	//commented out to use the exponent mask
	//"$basephongexponent" "15"
	//"$basephongboost" "3"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "2.25"
	"$rimmask" "1"
	

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}