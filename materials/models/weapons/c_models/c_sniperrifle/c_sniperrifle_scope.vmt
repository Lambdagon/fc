"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_models/c_sniperrifle/c_sniperrifle_scope"

	"$basemapalphaphongmask" "1"
	"$basealphaenvmapmask" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.4 .4 .4]"	

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "5"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$tempvar" "0"

	"$basephongexponent" "15"
	"$basephongboost" "5"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}