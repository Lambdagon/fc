"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_models/c_rustv/c_more_gun_blue"
	"$phongexponenttexture" "models/weapons/c_models/c_rustv/c_more_gun_exp"
	"$selfillummask" "models/weapons/c_models/c_rustv/c_more_gun_illum"
	"$phongalbedotint" "1"
	"$basemapalphaphongmask" "1"
	"$selfillum" "1"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "65"	
	"$lightwarptexture" "models/weapons/c_items/c_ambassador_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$rimlight" "1"
	"$rimlightexponent" "7"	
	"$rimlightboost" "15.25"
	"$selfillumtint" "[5 5 5]"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}



