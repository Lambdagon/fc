"VertexLitGeneric"
{
	"$basetexture" "models\maxxy\female_sniper/femsniperface_diffuse"
	"$bumpmap"     "models\maxxy\female_sniper/femsniperface_phongmask"
	"$stretch"   "models\maxxy\female_sniper/femsniperface_compress"
	"$compress"     "models\maxxy\female_sniper/femsniperface_stretch"
	"$bumpstretch" "models\maxxy\female_sniper/femsniperface_compress_n"
	"$bumpcompress" "models\maxxy\female_sniper/femsniperface_stretch_n"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "1"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$phong" "1"
	"$phongexponenttexture" "models\maxxy\female_sniper/femsniperface_exponent"
	"$phongboost" "0.3"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"					// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"			// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"			// Boost for ambient cube component of rim lighting
	"$rimmask" "1"                  // Use the alpha channel of the $phongexponenttexture as a rim mask.

	// Cloaking
	"$cloakPassEnabled" "1"
	"$cloakColorTint" "[1 0.5 0.4]"
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
	}
}
