"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_items/c_frying_pan_gold"

	"$phong" "1"
	"$phongexponent" "110"
	"$phongboost" "2"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"	
	"$phongfresnelranges"	"[.25 .5 4]"
	"$basemapalphaphongmask" "1"

	// Begin GOLD look

	"$envmap" "cubemaps/cubemap_gold001"	
	"$envmaptint" "[2.5 2.5 1.15]"
	
	// End GOLD look

	"$rimlight" "1"						
	"$rimlightexponent" "70"				
	"$rimlightboost" ".25"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}